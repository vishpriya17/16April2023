{
	"name": "parameterdataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "SotargeAzureDataLake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"dataset_container": {
				"type": "string"
			},
			"dataset_directory": {
				"type": "string"
			},
			"dataset_filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().dataset_filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().dataset_directory",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().dataset_container",
					"type": "Expression"
				}
			},
			"columnDelimiter": "\t",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}